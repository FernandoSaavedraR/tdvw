                     <effect>
                        <Blend mode="ADD" />
                     </effect>
                  </Label>
               </children>
               <StackPane.margin>
                  <Insets bottom="550.0" />
               </StackPane.margin>
               <effect>
                  <DropShadow />
               </effect>
            </HBox>
            <VBox fillWidth="false">
               <children>
                  <Label text="Nombre">
                     <VBox.margin>
                        <Insets left="80.0" top="20.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Descripcion">
                     <VBox.margin>
                        <Insets left="80.0" top="30.0" />
                     </VBox.margin>
                  </Label>
                  <Label text="Cantidad">
