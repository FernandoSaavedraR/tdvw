            </VBox>
            <VBox prefWidth="98.0">
               <children>
                  <TextField fx:id="nombre">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="descripcion">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </TextField>
                  <TextField fx:id="precio">
                     <VBox.margin>
                        <Insets top="15.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <StackPane.margin>
                  <Insets left="200.0" right="250.0" top="130.0" />
